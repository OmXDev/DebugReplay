generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Log {
  id           Int      @id @default(autoincrement())
  method       String
  url          String
  statusCode   Int
  requestBody  Json
  responseBody Json
  headers      Json
  duration     Int
  createdAt    DateTime @default(now())
}
